@{
    ViewBag.Title = "LowValue";
    Layout = "~/Views/Shared/_Content.cshtml";
}

<link href="~/Asset/CSS/LowValue.css" rel="stylesheet" />
<script src="~/Asset/js/LowValue/LowValue.js"></script>

<div>
    <a id="add_sheet" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-add'" onclick="showDialog()">登记</a>
    
    <a href="#" class="easyui-linkbutton" data-options="iconCls:'icon-save'" onclick="ExportExcel()">下载登记记录</a>
</div>
<div>
    <table id="sheet_list" class="easyui-datagrid">
        <thead>
            <tr>
                <th data-options="field:'sheet_No',width:50">验收单编号</th>
                <th data-options="field:'takeUnit',width:50">采购单位</th>
                <th data-options="field:'taker',width:50">领用人</th>
                <th data-options="field:'sumPrice',width:50">合计金额</th>
                <th data-options="field:'Operate',formatter:Operate">操作</th>
            </tr>
        </thead>
    </table>
</div>



<div id="lowValueSheet" class="easyui-window" title="课程添加" style="text-align:center" data-options="iconCls:'icon-tip',modal:true,closed:true">
    <div id="head">
        <span id="sheetNo">0000001</span>
        <div style="width:677px;">
            <h2 class="center_class">南京森林警察学院</h2>
            <h1 class="center_class">低值耐用品及耗材验收单</h1>
        </div>
        <hr class="center_class" /><hr class="center_class" />

    </div>
    <div id="body">
        <div style="margin-bottom:5px;">
            <span>领物单位：<input type="text" id="takeUnit" /></span>
            <span style="padding-left:236px;">验收日期<input id="checkTime" type="text" class="easyui-datebox" /></span>
        </div>

        <div>
            <table id="sheet" border="1" cellpadding="0" cellspacing="0">
                <tr>
                    <td id="noHead">编号</td>
                    <td id="nameHead">名称</td>
                    <td id="modelHead">规格型号</td>
                    <td class="small">单位</td>
                    <td class="small">数量</td>
                    <td class="small">单价</td>
                    <td id="totalPriceHead">金额（元）</td>
                </tr>
                <tr>
                    <td><input type="text" id="no1" /></td>
                    <td><input type="text" id="name1"></td>
                    <td><input type="text" id="model1"></td>
                    <td><input type="text" id="unit1"></td>
                    <td><input type="text" id="qty1"></td>
                    <td><input type="text" id="price1"></td>
                    <td><input type="text" id="totalPrice1"></td>
                </tr>
                <tr>
                    <td><input type="text" id="no2"></td>
                    <td><input type="text" id="name2"></td>
                    <td><input type="text" id="model2"></td>
                    <td><input type="text" id="unit2"></td>
                    <td><input type="text" id="qty2"></td>
                    <td><input type="text" id="price2"></td>
                    <td><input type="text" id="totalPrice2"></td>
                </tr>
                <tr>
                    <td><input type="text" id="no3"></td>
                    <td><input type="text" id="name3"></td>
                    <td><input type="text" id="model3"></td>
                    <td><input type="text" id="unit3"></td>
                    <td><input type="text" id="qty3"></td>
                    <td><input type="text" id="price3"></td>
                    <td><input type="text" id="totalPrice3"></td>
                </tr>
                <tr>
                    <td><input type="text" id="no4"></td>
                    <td><input type="text" id="name4"></td>
                    <td><input type="text" id="model4"></td>
                    <td><input type="text" id="unit4"></td>
                    <td><input type="text" id="qty4"></td>
                    <td><input type="text" id="price4"></td>
                    <td><input type="text" id="totalPrice4"></td>
                </tr>
                <tr>
                    <td rowspan="2">附注</td>
                    <td colspan="2">其他低值耐用品及低值易耗品等（附清单）</td>
                    <td colspan="3" rowspan="2">合计</td>
                    <td rowspan="2"><input type="text" id="sumPrice" /></td>
                </tr>
                <tr>
                    <td>总额合计（元）</td>
                    <td>￥：<input type="text" style="width:157px;" id="attachmentTotalPrice" /></td>
                </tr>
            </table>
        </div>

        <div style="margin-top:5px;">
            <span>部门负责人:<input id="chairman" type="text" size="6" /></span>
            <span style="padding-left:20px;">采购人:<input id="buyer" type="text" size="6" /></span>
            <span style="padding-left:20px;">领用人:<input id="taker" type="text" size="6" /></span>
            <span style="padding-left:20px;">验收人:<input id="checker" type="text" size="6" /></span>
        </div>
    </div>
    <br />
    <a id="save_sheet" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-save'" onclick="saveLowValue()">保存</a>
</div>

