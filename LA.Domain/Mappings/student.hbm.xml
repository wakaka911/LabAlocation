<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="LA.Domain" assembly="LA.Domain">

  <class name="Student" table="Student">
    <id name="ID" type="String" unsaved-value="null">
      <column name="ID" sql-type="varchar" not-null="true"/>
      <generator class="assigned" />
    </id>
    <property name="CREATED" type="DateTime">
      <column name="CREATED" sql-type="datetime" not-null="false"/>
    </property>
    <property name="UPDATED" type="DateTime">
      <column name="UPDATED" sql-type="datetime" not-null="false"/>
    </property>
    <property name="studentName" type="String">
      <column name="studentName" sql-type="nvarchar" not-null="false"/>
    </property>
    <property name="studentNo" type="String">
      <column name="studentNo" sql-type="varchar" not-null="false"/>
    </property>
    <property name="studentSex" type="int">
      <column name="studentSex" sql-type="int" not-null="false"/>
    </property>
    <property name="studentSession" type="String">
      <column name="studentSession" sql-type="varchar" not-null="false"/>
    </property>
    <property name="studentProfession" type="String">
      <column name="studentProfession" sql-type="nvarchar" not-null="false"/>
    </property>
    <property name="studentClass" type="String">
      <column name="studentClass" sql-type="varchar" not-null="false"/>
    </property>
    <property name="account" type="string">
      <column name="account" sql-type="varchar" not-null="false"/>
    </property>


  </class>


</hibernate-mapping>